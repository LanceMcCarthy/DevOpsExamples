      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/MauiDemo.csproj",
        "-f",
        "net10.0-android",
        "-c",
        "Debug"
      ],
      "group": "build",
      "options": {
        "env": {
          "TELERIK_USERNAME": "${env:TELERIK_USERNAME}",
          "TELERIK_PASSWORD": "${env:TELERIK_PASSWORD}"
        }
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": true,
        "clear": false
      },
      "problemMatcher": "$msCompile"
