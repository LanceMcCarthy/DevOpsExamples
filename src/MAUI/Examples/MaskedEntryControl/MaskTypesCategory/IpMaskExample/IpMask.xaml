<?xml version="1.0" encoding="utf-8" ?>
<telerik:RadContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                        xmlns:telerik="clr-namespace:Telerik.Maui.Controls;assembly=Telerik.Maui.Controls"
                        x:Class="SDKBrowserMaui.Examples.MaskedEntryControl.MaskTypesCategory.IpMaskExample.IpMask"
                        Margin="5">
    <telerik:RadContentView.Content>
        <ScrollView>
            <VerticalStackLayout>

                <Label Text="Default IP Mask:" Margin="0, 10, 0, 0" />
                <telerik:RadIPMaskedEntry/>

                <Label Text="IP Mask with changed PropmtChar:" Margin="0, 10, 0, 0"  />
                <telerik:RadIPMaskedEntry PromptChar="#"/>

            </VerticalStackLayout>
        </ScrollView>
    </telerik:RadContentView.Content>
</telerik:RadContentView>